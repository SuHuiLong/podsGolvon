#import <Foundation/Foundation.h>

@interface DynamicMessageModel : NSObject


@property (copy, nonatomic) NSString *type;                     //0为留言, 1为回复

@property (copy, nonatomic) NSString *cid;                      //评论id

@property (copy, nonatomic) NSString *nickname;                 //昵称

@property (copy, nonatomic) NSString *comuid;                   //评论人的id

@property (copy, nonatomic) NSString *avator;                   //评论人的头像

@property (copy, nonatomic) NSString *content;                  //内容

@property (copy, nonatomic) NSString *covavator;                //回复人的头像

@property (copy, nonatomic) NSString *time;                     //时间

@property (copy, nonatomic) NSString *covuid;                   //被回复人的id

@property (copy, nonatomic) NSString *covcid;                   //被回复评论的ID

@property (copy, nonatomic) NSString *covnickname;              //被回复人的头像

@property (copy, nonatomic) NSString    *interview;

@end
